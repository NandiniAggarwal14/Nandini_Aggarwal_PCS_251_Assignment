#include<stdio.h>
int main()
{
    int m,n,a[20][20],i,j,s=0;
    printf("\nEnter number of rows and columns");
    scanf("%d%d",&m,&n);
    printf("\nEnter array elements");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<m;i++)
    {
        s=s+a[i][i];
    }
    printf("Sum is %d",s);
    return 0;
}

/*      INPUT
Enter number of rows and columns3
3

Enter array elements1
2
3
4
5
6
7
8
9

      OUTPUT
      Sum is 15
*/
